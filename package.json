{
    "name": "cheddar",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky install",
        "check-ts": "tsc --noEmit --project tsconfig.json",
        "check-format": "prettier --list-different .",
        "format": "prettier --write .",
        "pre-commit": "yarn check-ts && lint-staged"
    },
    "lint-staged": {
        "**/*.+(js|jsx|ts|tsx)": [
            "prettier --write",
            "eslint"
        ]
    },
    "dependencies": {
        "@prisma/client": "^3.10.0",
        "@types/bcrypt": "^5.0.0",
        "@types/jsonwebtoken": "^8.5.8",
        "bcrypt": "^5.0.1",
        "jsonwebtoken": "^8.5.1",
        "next": "12.1.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/node": "17.0.21",
        "@types/react": "17.0.39",
        "eslint": "8.10.0",
        "eslint-config-next": "12.1.0",
        "husky": "^7.0.0",
        "lint-staged": "^12.3.5",
        "prettier": "2.5.1",
        "prisma": "^3.10.0",
        "typescript": "4.6.2"
    }
}
